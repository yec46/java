package sub1;
/*
 * 날짜:2019.10.15
 * 이름:박성
 * 내용:다차원 배열 실습하기
 */

public class ArraryTest2 {
	public static void main(String[] args) {
		//1차원 배열
		int[] score = {80, 60, 78, 69, 87, 92};
		
		int sum = 0;
		
		for(int i=0; i<6; i++) {
			sum += score[i];
		}
				
		System.out.println("점수의 총합: "+sum);
		//2차원 배열
		
	}

}
